<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.annaroxas.teamdiscovery">

    <supports-screens
        android:largeScreens="true"
        android:smallScreens="false"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".System">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Create_user"
            android:parentActivityName=".System" />
        <activity android:name=".TestGame" />
        <activity
            android:name=".testXML"
            android:label="@string/title_activity_test_xml"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".SuccessLogin" />
    </application>

</manifest>